<?xml version='1.0' encoding='utf-8'?>
<widget id="org.arg.discount.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>argDiscountApp</name>
    <description>
        No Description.
    </description>
    <author email="ingjeanpaulrojas@gmail.com" href="http://ingjprojas.zz.com.ve">
        Jean Paul Rojas
    </author>
    <content src="index.html" />
    <icon src="www/img/favicon.png" />
    <access origin="*" />
    <allow-navigation href="http://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <splash src="splash1.jpg" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="fullscreen" value="true" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="android-targetSdkVersion" value="29" />
    <preference name="SplashScreenBackgroundColor" value="rgb(81,45,168)" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashScreenWidth" value="750" />
    <preference name="SplashScreen" value="splash2.jpg" />
    <preference name="SplashScreenHeight" value="1000" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="3000" />
</widget>
